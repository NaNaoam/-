#include <stdio.h>
#include <stdlib.h>


typedef struct NODE {

	int data;
	struct NODE* next; //다름 노드의 주소를 저장할 자기참조 구조체포인터


}node;

int Insert(){
    int data; 
    printf("데이터를 입력하세요 : \n");
    scanf("%d", &data);

    return data;

}
void Print_list (struct NODE *head){

node* curr = head->next; //순회용 노드

    printf("List is : ");
	while (curr != NULL) {
        
		printf("%d ", curr->data);
		curr = curr->next;
	}printf("\n");



    
}
int main(void) {

	node* head = (node*)malloc(sizeof(node));//헤드(시작) 노드 생성

	head->next = NULL;//머리노드의 next 값은 null

	node* node1 = (node*)malloc(sizeof(node));

	head->next = node1->next;
	node1->data = Insert();
	head->next = node1;

	node* node2 = (node*)malloc(sizeof(node));
	node2->next = node1->next;
	node2->data = Insert();
	node1->next = node2;
	
    Print_list(head);
	

	free(head);
	free(node1);
	free(node2);
	
 return 0;
}
